public class Main {
  int x = 5;

  public static void main(String[] args) {
    Main myObj = new Main();
    System.out.println(myObj.x);
  }
}
public class Main {
  public static void main(String[] args) {
    int x = 5;
    int y = 6;
    int sum = x + y;
    System.out.println(sum); // Print the sum of x + y
  }
}
